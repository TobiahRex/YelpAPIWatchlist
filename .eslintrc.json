{
	"extends": "airbnb",
	"rules": {
		"no-underscore-dangle": ["error", { "allow": ["_id"] }],
		"no-param-reassign": ["error", { "props": false }],
		"no-use-before-define": ["error", { "functions": false }],
		"no-use-before-define": ["error", { "classes": false }]
	},
	"globals": {
		"$": false,
		"dbUser._id": true,
		"angular": true,
		"Yelp": true,
		"User": true
	},
	"env": {
		"browser": true,
		"node": true,
		"es6": true,
		"jquery": true
	}
}
